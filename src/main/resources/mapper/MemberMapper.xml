<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.landongnet.auth.mapper.MemberMapper">

      <!-- 通用查询映射结果 -->
      <resultMap id="BaseResultMap" type="com.landongnet.auth.model.entity.Member">
                <id column="id" property="id"/>
                <result column="member_id" property="memberId"/>
                <result column="phone" property="phone"/>
                <result column="avatar" property="avatar"/>
                <result column="nickname" property="nickname"/>
                <result column="gender" property="gender"/>
                <result column="type" property="type"/>
                <result column="register_time" property="registerTime"/>
                <result column="create_time" property="createTime"/>
                <result column="modify_time" property="modifyTime"/>
                <result column="remark" property="remark"/>
      </resultMap>

      <!-- 通用查询结果列 -->
      <sql id="Base_Column_List">
          id, member_id, phone, avatar, nickname, gender, type, register_time, create_time, modify_time, remark
      </sql>

</mapper>
